{
  "name": "rfq-local-buying-program",
  "version": "0.1.0",
  "private": true,
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "npm run lint:fix",
      "git add --force"
    ],
    "**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ],
    "*.{css,less,styl,scss,sass,sss}": [
      "stylelint --fix",
      "git add --force"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "db:migrate": "sequelize db:migrate",
    "format": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|yml|yaml|css|md)\"",
    "lint": "eslint \"**/*.+(js|jsx|json|ts|tsx)\"",
    "lint:fix": "npm run lint -- --fix --quiet",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test": "lint-staged",
    "test:ci": "npm run cypress:run",
    "test:ci:record": "npm run cypress:run -- --record",
    "prepare": "husky install"
  },
